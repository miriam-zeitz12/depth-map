<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.ncf.miriam_zeitz12.depthmap" >
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HelpScreenActivity"
            android:label="@string/title_activity_help_screen" >
        </activity>
        <activity
            android:name=".MoreInfoActivity"
            android:label="@string/title_activity_more_info" >
        </activity>
        <activity
            android:name=".EmailFileActivity"
            android:label="@string/title_activity_email_file" >
        </activity>
        <activity
            android:name=".MeshActivity"
            android:label="@string/title_activity_mesh" >
        </activity>
    </application>

</manifest>
